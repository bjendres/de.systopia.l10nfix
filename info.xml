<?xml version="1.0"?>
<extension key="de.systopia.l10nfix" type="module">
  <file>l10nfix</file>
  <name>Localisation Fixes</name>
  <description>Fixes certain known localisation issues that cannot be fixed within the CiviCRM Core</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.l10nfix</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.l10nfix</url>
    <url desc="Support">https://github.com/systopia/de.systopia.l10nfix/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>0.1</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments/>
  <requires>
    <ext>org.civicrm.l10nx</ext>
  </requires>
  <civix>
    <namespace>CRM/L10nfix</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
